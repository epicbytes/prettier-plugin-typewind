{
	"compilerOptions": {
		"target": "es6",
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"declaration": true,
		"module": "commonjs",
		"outDir": "lib",
		"moduleResolution": "node",
		"noImplicitAny": false
	},
	"include": ["src"],
	"exclude": ["lib", "node_modules"]
}
